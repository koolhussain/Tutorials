<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="struts2" extends="struts-default">
		<action name="reg" class="struts2.RegisterAction">
			<result name="s">registerSuccess.jsp</result>
			<result name="f">registerFailure.jsp</result>
			<result name="input">/register.jsp</result>
		</action>
		<action name="login" class="struts2.LoginAction">
			<result name="s">success.jsp</result>
			<result name="f">failure.jsp</result>
			<result name="input">/login.jsp</result>
		</action>
	</package>
</struts>